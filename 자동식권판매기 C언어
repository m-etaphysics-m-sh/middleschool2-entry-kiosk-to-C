 //엔트리로 만들었던 자동 식권 판매기 코드
#include <stdio.h>
int main() {

	printf("투입 금액을 입력하세요\n");
	int a;
	scanf_s("%d", &a);
	printf
	("김밥: 2000원\n김치찌개: 5000원\n볶음밥:4000원\n초밥: 3000원\n");
	printf("구매하실 상품의 개수를 각각 입력해주세요\n");
	
	int b, c, d, e;
	scanf_s("%d %d %d %d", &b, &c, &d, &e);

	if (a >= b*2000 + c*5000 + d*4000 + e*3000) {
		printf("결제가 완료되었습니다.");
	}

	else {
		printf("투입 금액이 부족합니다.\n결제에 실패하였습니다.\n");
	}
}
